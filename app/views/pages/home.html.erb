<div class="flex-container">

  <%# <div class=""> %>
  <h4 class="h4-home"><strong>Themes</strong></h4>

  <div class="container themes-card-container">
    <% @themes.each do |theme| %>
      <%= link_to theme_path(theme) do %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path theme.photo.key %>)">
          <h3><%= theme.title.truncate(20) %></h3>
          <p><%= theme.description.truncate(70) %></p>
        </div>
      <% end %>
    <% end %>
  </div>


  <div class="flex-item">
    <div class="calendar-container">
    <%= image_tag "calendar.png", alt: "calendar", width: 300 %>
    <%= image_tag "calendar.png", alt: "calendar", width: 300 %>
    </div>

  </div>


</div>
